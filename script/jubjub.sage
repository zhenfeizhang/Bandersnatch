
p=52435875175126190479447740508185965837690552500527637822603658699938581184513
Fp=Zmod(p)


#####################################################
# Twised Edwards curve: a * x² + y² = 1 + d * x² * y²
#####################################################
TEa=Fp(-1)
TEd=Fp(19257038036680949359750312669786877991949435402254120286184196891950884077233)

# TE curve generator
TEx = Fp(8076246640662884909881801758704306714034609987455869804520522091855516602923)
TEy = Fp(13262374693698910701929044844600465831413122818447359594527400194675274060458)

assert(TEa * TEx^2 + TEy^2 - 1 - TEd * TEx^2 * TEy^2 == 0)

#####################################################
# Montgomery curve: By² = x³ + A * x² + x
#####################################################
MB=Fp(-40964)
MA=Fp(40962)

assert((TEa +TEd)*2/(TEa-TEd) == MA)
assert(4/(TEa-TEd) == MB )

# M curve generator
Mx = (1+TEy)/(1-TEy)
My = Mx/TEx


assert(MB * My^2 - Mx^3 - MA * Mx^2 - Mx == 0)

#####################################################
# Weierstrass curve: y² = x³ + A * x + B
#####################################################
WA=Fp(52296097456646850916096512823759002727550416093741407922227928430486925478210)
WB=Fp(48351165704696163914533707656614864561753505123260775585269522553028192119009)

assert((3-MA^2)/3/MB^2 == WA)
assert((2*MA^3-9*MA)/27/MB^3 == WB)

Wx = Mx/MB + MA/3/MB
Wy = My/MB

assert(Wy^2 - Wx^3 - WA * Wx - WB == 0)